{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*",
        "static/*"
      ]
    },
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noEmit": true
  }
}
